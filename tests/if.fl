fn check(a: int) -> int {
	if a == 0 {
		return 0
	}

	elif a == 1 {
		return 1
	}

	else {
		return 2
	}
}

assert check(0) == 0
assert check(1) == 1
assert check(2) == 2
assert check(3) == 2

